{
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint-recommended",
    "eslint-react",
    "eslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-debugger": 0,
    "no-console": 0,
    "jsx-no-lambda": false,
    "no-unused-vars":"error"
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}